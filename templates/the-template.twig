{% extends 'base.twig' %}

{% block content %}

	{% for item in post.get_field('page_content') %}
		{% if item.acf_fc_layout == 'section' %}
			{% include 'partials/section.twig' with { 'item': item } %}
		{% elseif item.acf_fc_layout == 'jumbotron' %}
			{% include 'partials/jumbotron.twig' with { 'item': item } %}
		{% endif %}
	
	{% endfor %}
{% endblock %}